{
  "name": "MockingJay",
  "version": "1.0.0",
  "author": "Abhaya Rawal",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "build-scss": "node-sass --include-path scss web/scss/core.scss web/css/app.css",
    "watch-css": "nodemon -e scss -x \"npm run build-scss\"",
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" \"npm run watch-css\""
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "^2.0.0-beta.13",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.4",
    "systemjs": "^0.19.25",
    "zone.js": "^0.6.8"
  },
  "devDependencies": {
    "concurrently": "^1.0.0",
    "lite-server": "^1.3.1",
    "typescript": "^1.7.3",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1"
  }
}
